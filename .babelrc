{
  "presets": [
    ["env", { "modules": false }],
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-runtime",
    ["import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-class-properties",
        "transform-decorators-legacy",
        "transform-runtime"
      ]
    }
  }
}